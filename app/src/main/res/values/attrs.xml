<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">
        <attr name="rpi_round_color" format="color"/>
        <attr name="rpi_round_progress_color" format="color"/>
        <attr name="rpi_round_width" format ="dimension" />
        <attr name="rpi_text_color" format="color"/>
        <attr name="rpi_text_size" format="dimension"/>
        <attr name="rpi_max" format="integer"/>
        <attr name="rpi_textIsDisplayable" format = "boolean"/>
        <attr name="style">
            <enum name="STROKE" value ="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>